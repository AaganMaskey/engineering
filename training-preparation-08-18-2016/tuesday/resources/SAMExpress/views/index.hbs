<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	<h1>Excel Upload</h1>
	<input type="file" id="file">

	<script src="js/jquery.js"></script>
	<script>
		$('#file').change(function (e){

			var file = e.target.files[0];

			var formData = new FormData();
		    formData.append('myfile',file);
		    
		    $.ajax({
		        url:'excel',
		        type: 'post',
		        data: formData,
		        processData: false,
		        contentType: false,
		        success:function(res){
		            
		        },
		        error: function(res){
		            
		        },
		        crossDomain: true
		    });
		});
	</script>
		
</body>
</html>